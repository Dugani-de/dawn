  /*-_- -_- -_- Buttons -_- -_- -_-*/
  .st_Btn {
    cursor: pointer;
    transition: all 0.3s ease-in-out 0s;
  }

  .st_Btn:hover {
    scale: 1.1;
  }

  .st_Btn:active {
    scale: 0.9;
    opacity: 0.3;
    transition: none;
  }

  /*-_- -_- -_- Swiper -_- -_- -_-*/
  .st_Insta-Popup-Carousel .swiper-button-next-sternify,
  .st_Insta-Popup-Carousel .swiper-button-prev-sternify {
    position: absolute;
    width: 50px;
    height: 50px;
    top: var(--swiper-navigation-top-offset, 50%);
    border: 1px solid hsla(0, 0%, 13%, 0.09019607843137255);
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: -20px;
    cursor: pointer;
    box-shadow: rgba(0, 0, 0, 0.1) 0px 10px 50px;
    background-color: #ffffff9e;
    z-index: 10;
  }

  .st_Insta-Popup-Carousel .swiper-button-next-sternify::after,
  .st_Insta-Popup-Carousel .swiper-button-prev-sternify::after {
    content: none;
  }

  .st_Insta-Popup-Carousel .swiper-button-next-sternify {
    transform: rotate(180deg);
  }

  .st_Insta-Popup-Carousel .swiper-button-next-sternify:hover,
  .st_Insta-Popup-Carousel .swiper-button-prev-sternify:hover {
    scale: 1.1;
    opacity: 0.5;
    transition: all 300ms ease-in-out 0s;
  }

  .st_Insta-Popup-Carousel .swiper-button-next-sternify:active,
  .st_Insta-Popup-Carousel .swiper-button-prev-sternify:active {
    scale: 0.9;
    opacity: 1;
    transition: 0s;
  }

  .st_Insta-Popup-Carousel .swiper-button-next-sternify svg,
  .st_Insta-Popup-Carousel .swiper-button-prev-sternify svg {
    width: 50px !important;
    object-fit: contain;
  }

  .st_Insta-Popup-Carousel .swiper-button-next-sternify {
    left: auto;
    right: 10px;
  }

  .st_Insta-Popup-Carousel .swiper-button-prev-sternify {
    left: 10px;
    right: auto;
  }

  .swiper {
    width: 100%;
    max-height: 100%;
