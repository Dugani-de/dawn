  document.addEventListener('DOMContentLoaded', function () {
    document.querySelectorAll('.st_Insta-Gallery-Box').forEach((element) => {
      element.addEventListener('click', function () {
        document.querySelector('.st_Insta').classList.add('st_Show_PopUp');
      });
    });

    document.querySelectorAll('.st_Insta-Popup-BackBlur').forEach((element) => {
      element.addEventListener('click', function () {
        document.querySelector('.st_Insta').classList.remove('st_Show_PopUp');
      });
    });

    document.querySelectorAll('.st_Insta-Popup-Close-Btn').forEach((element) => {
      element.addEventListener('click', function () {
        document.querySelector('.st_Insta').classList.remove('st_Show_PopUp');
      });
    });

    const swiper = new Swiper('.st_Insta-Popup-Carousel .swiper', {
      loop: true,
      speed: 500,
      spaceBetween: 50,
      grabCursor: true,
      navigation: {
        nextEl: '.swiper-button-next-sternify',
        prevEl: '.swiper-button-prev-sternify',
      },
    });
  });
</script>

